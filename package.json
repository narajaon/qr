{
  "name": "closedsauce",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "parallel --halt now,fail=2 --ungroup ::: 'next dev' 'NODE_ENV=development postcss ./src/styles/globals.css -o ./build/tailwind.css'",
    "build": "NODE_ENV=production yarn postcss ./src/styles/*  -o ./build/tailwind.css && yarn next build",
    "start": "next start",
    "lint": "yarn prettier --write src/**"
  },
  "dependencies": {
    "next": "10.2.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "3.17.0"
  },
  "devDependencies": {
    "@types/react": "17.0.11",
    "autoprefixer": "10.2.5",
    "graphql": "15.5.0",
    "postcss": "8.2.15",
    "postcss-cli": "8.3.1",
    "prettier": "2.3.1",
    "tailwindcss": "2.1.2",
    "typescript": "4.3.2"
  }
}
